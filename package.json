{
  "name": "ktx-hdr-viewer",
  "displayName": "KTX-HDR Viewer",
  "description": "preview ktx, hdr files",
  "icon": "./asset/logo.png",
  "version": "0.0.1",
  "publisher": "ybt",
  "engines": {
    "vscode": "^1.98.0"
  },
  "author": {
		"name": "ybt",
		"email": "651708784@qq.com"
	},
  "repository": {
    "type": "git",
    "url": "https://github.com/ybt-new/ktx-hdr-viewer"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "keywords": [
    "hdr",
    "ktx"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "ktx-hdr.preview",
        "displayName": "ktx-hdr",
        "selector": [
          {
            "filenamePattern": "*.{ktx,ktx2,hdr}"
          }
        ]
      }
    ]
  },
  "scripts": {
    "release": "pnpm vsce publish --no-dependencies",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/three": "^0.175.0",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "three": "^0.175.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}